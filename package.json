{
  "name": "base-boilerplate",
  "description": "Plugin that adds support for generating project files from a declarative boilerplate configuration.",
  "version": "0.2.1",
  "homepage": "https://github.com/node-base/base-boilerplate",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "node-base/base-boilerplate",
  "bugs": {
    "url": "https://github.com/node-base/base-boilerplate/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE",
    "README.md",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "async-each-series": "^1.1.0",
    "base-pipeline": "^0.3.2",
    "base-plugins": "^0.4.13",
    "base-scaffold": "^0.3.2",
    "debug": "^2.2.0",
    "is-boilerplate": "^0.1.1",
    "is-valid-app": "^0.2.0",
    "kind-of": "^3.0.3",
    "lazy-cache": "^2.0.1",
    "merge-stream": "^1.0.0",
    "mixin-deep": "^1.1.3"
  },
  "devDependencies": {
    "base-app": "^0.2.6",
    "boilerplate": "^0.6.0",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3"
  },
  "keywords": [
    "api",
    "app",
    "application",
    "base",
    "baseplugin",
    "boilerplate",
    "bp",
    "building-blocks",
    "config",
    "configuration",
    "create",
    "declarative",
    "files",
    "framework",
    "generate",
    "plugin",
    "plugins",
    "scaffold",
    "scaffolding",
    "skeleton",
    "tool",
    "toolkit",
    "tools"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "base",
        "base-generators",
        "base-scaffold",
        "base-task"
      ]
    },
    "reflinks": [
      "boilerplate",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
